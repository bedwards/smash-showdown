{
  "name": "FaultlineFear",
  "tree": {
    "$className": "DataModel",

    "ServerStorage": {
      "$className": "ServerStorage",
      "AssetTemplates": {
        "$className": "Folder"
      }
    },

    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "Shared": {
        "$path": "src/faultline-fear/shared"
      }
    },

    "ServerScriptService": {
      "$className": "ServerScriptService",
      "Server": {
        "$path": "src/faultline-fear/server"
      }
    },

    "StarterPlayer": {
      "$className": "StarterPlayer",
      "StarterPlayerScripts": {
        "$className": "StarterPlayerScripts",
        "Client": {
          "$path": "src/faultline-fear/client"
        }
      }
    },

    "Workspace": {
      "$className": "Workspace",
      "$properties": {
        "StreamingEnabled": true
      },
      "SpawnLocation": {
        "$className": "SpawnLocation",
        "$properties": {
          "Position": [0, 10, -1000],
          "Size": [10, 1, 10],
          "Anchored": true,
          "Transparency": 1,
          "CanCollide": false,
          "Neutral": true,
          "Duration": 0
        }
      }
    },

    "Lighting": {
      "$className": "Lighting",
      "$properties": {
        "Ambient": [0.1, 0.1, 0.12],
        "Brightness": 2,
        "ClockTime": 14,
        "GeographicLatitude": 37.7749,
        "OutdoorAmbient": [0.2, 0.2, 0.22],
        "Technology": "Voxel"
      }
    },

    "SoundService": {
      "$className": "SoundService",
      "$properties": {
        "AmbientReverb": "Forest"
      }
    },

    "HttpService": {
      "$className": "HttpService",
      "$properties": {
        "HttpEnabled": true
      }
    }
  }
}
